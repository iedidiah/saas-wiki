<h1>My Wikis</h1>
<div class="row">
  <div class="col-md-8">
    <%= render partial: "wikis/wikis", collection: @wikis %>
  </div>
  
  <div class="col-md-4">
    <h3>Create a new Wiki</h3>
        <%= form_for @new_wiki do |f| %>
          <div class="form-group">
          <%= f.label :title %>
          <%= f.text_field :title, class: 'panel-title'%>
        </div>
        <div class="form-group">
          <%= f.text_area :body, rows: 8, cols: 42, class: 'panel-body' %>
        </div>
        <div class="form-group">
          <%=f.submit "Save", class: "btn btn-primary" %>
        </div>
        <% end %>
  </div><!-- end of col -->
</div>
