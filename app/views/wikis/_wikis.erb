    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">
          <%= wikis.title %>
        </h3>
      </div>
      <div class="panel-body">
        <%= wikis.body %>
      </div>
      <div class="panel-footer">
        submitted <%= time_ago_in_words(wikis.created_at) %> ago by <%= wikis.user.user_name %>,<% if wikis.public %> Public<% else %> Non-public<% end %> Wiki
        <%= link_to "Edit", edit_wiki_path(wikis), class: 'btn btn-success' %> 
        <%= link_to "Delete Wiki", wikis, method: :delete, class: 'btn btn-danger pull-right', data: { confirm: 'Do you want to delete this Wiki?' } %>
      </div>
    </div>
