<h1>Here are all our Public Wikis</h1>
<% @wikis.each do |wiki| %>
<div class="row">
  <div class="col-md-8">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">
          <%= wiki.title %>
        </h3>
      </div>
      <div class="panel-body">
        <%= wiki.body %>
      </div>
      <div class="panel-footer">
        submitted <%= time_ago_in_words(wiki.created_at) %> ago by <%=  wiki.creator.user_name %>     
      </div>
    </div>
  </div>
</div>
<% end %>
